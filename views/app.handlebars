<html lang="en">
<head>
  <meta charset="utf-8"/>
  <title>Task Maker</title>
  <script crossorigin src="https://unpkg.com/react@17/umd/react.development.js"></script>
  <script crossorigin src="https://unpkg.com/react-dom@17/umd/react-dom.development.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/babel-core/5.8.34/browser.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.2.0/jquery.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/remarkable/1.6.2/remarkable.min.js"></script>
  <link rel="shortcut icon" href="/assets/img/favicon.png"/>
  <link rel="stylesheet" type="text/css" href="/assets/style.css"/>
</head>
<body>
  <nav><a href="/login"><img id="logo" src="/assets/img/favicon.png" alt="Calendar Icon"/></a>
    <div id="welcomeMessage"></div>
    <div class="navlink"><a href="/logout">Log out</a></div>
    <div class="navlink"><a id="changePassButton" href="/changePass" >Change Password</a></div>
    <div class="navlink"><a id="changeUsernameButton" href="/changeUsername" >Change Username</a></div>
  </nav>
  <div id="appWrapper">
    <img id="ad1" src="/assets/img/ad.png" alt="Placeholder Ad"/>
    <img id="ad2" src="/assets/img/ad.png" alt="Placeholder Ad"/>
    <section id="makeTask">
    </section>
    <div id="status">
      <h3>Try adding a task!</h3>
    </div>
    <section id="tasks">
    </section>
    <div id="taskMessage"><img id="speech" src="/assets/img/speech.png" alt="speech box"/>
      <h3><span id="errorMessage"></span></h3>
    </div>
  </div>
  <script src="/assets/bundle.js"></script>
</body>
</html>